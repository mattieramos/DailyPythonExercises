day_of_week = int(input("Type a number 1-7: "))
if day_of_week == 1:
    print("Sunday")
elif day_of_week == 2:
    print("Monday")
elif day_of_week == 3:
    print("Tuesday")
elif day_of_week == 4:
    print("Wednesday")
elif day_of_week == 5:
    print("Thursday")
elif day_of_week == 6:
    print("Friday")
elif day_of_week == 7:
    print("Saturday")
else:
    print("Number entered must be between 1-7")
